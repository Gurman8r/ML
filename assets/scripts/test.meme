# Test Script
# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

TEST = nodeid();
{
	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #
	
	myStruct = $();
	{
		foo = 123;
	};
	
	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

	main = [](args);
	{
		argc = sizeof(args);

		for (i = 0; i < argc; i = i + 1);
		{
			printl(args[i]);
		};
		
		printl(myStruct.foo);

		return true;
	};

	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

	return main(__ARGS__);
	
	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #
};